div#main h2{font-size:18px;font-weight:100;margin:20px 0 0 10px}h2 span{font-size:14px}div.uploadDiv{background:url(../Image/BGupload.png) left top no-repeat;height:40px;margin:20px 0 0 40px;overflow:hidden;position:relative;width:145px}input.file{position:absolute;top:0;left:0;cursor:pointer;opacity:0;filter:alpha(opacity : 0);z-index:999;height:40px;width:145px}input#upload,input#upload2,input#upload3{display:none}a.down,a.up{margin:3px 20px;display:block;width:32px;height:32px}a.down span,a.up span{display:none}a.down{background:url(../Image/upAndDown.png) -3px -40px no-repeat}a.up{background:url(../Image/upAndDown.png) -3px -5px no-repeat}a.down:hover{background:url(../Image/upAndDown.png) -3px -114px no-repeat}a.up:hover{background:url(../Image/upAndDown.png) -3px -78px no-repeat}a.delete{margin:0 20px;color:#1169BF;background:url(../Image/adminIconList.png) -6px -574px no-repeat;padding-left:25px}a.delete:hover{text-decoration:underline}table{margin-left:90px;margin-top:20px}table tr{margin-top:10px}div.marginDiv{margin:20px 0 0 40px}span.sm-advise{color:#888;font-size:12px;padding-left:10px}