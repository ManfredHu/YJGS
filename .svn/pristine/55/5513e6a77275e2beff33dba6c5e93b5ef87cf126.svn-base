*{margin:0;padding:0;border:0}body{line-height:1;font-family:"Microsoft Yahei",arial,"宋体";font-size:16px;background:url(../Image/body_bg.jpg) repeat-x #D5D5E1}ol,ul{list-style:none}a{text-decoration:none}form div a:hover.forget,p.noLoginTipsLink a:hover{text-decoration:underline}form{width:422px;margin:140px auto}form h1{font-size:22px;font-weight:100;line-height:45px;height:45px;text-align:center;color:#fff;border-radius:5px 5px 0 0;-moz-border-radius:5px 5px 0 0;background:#5c9dd7;background:-moz-linear-gradient(top,#5c9dd7,#4a8ac8);background:-webkit-gradient(linear,top,100% 50%,from(#5c9dd7),to(#4a8ac8));background:-webkit-linear-gradient(top,#5c9dd7,#4a8ac8);background:-o-linear-gradient(top,#5c9dd7,#4a8ac8);filter:progid:DXImageTransform.Microsoft.gradient(GradientType=0,
		startColorstr=#5c9dd7, endColorstr=#4a8ac8);-ms-filter:"progid:DXImageTransform.Microsoft.gradient (GradientType=0, startColorstr=#5c9dd7, endColorstr=#4a8ac8)"}form div.loginDiv{padding:30px 40px;background:#fff;position:relative;border-radius:0 0 5px 5px;height:270px}#checkNum,#error,#login,form div a.forget{position:absolute}#userNum,#userPwd{width:283px;border-radius:3px;-moz-border-radius:3px;margin-bottom:15px;outline:0}#userNum,#userPwd,#yanzhengma{height:21px;line-height:21px;font-size:16px;padding:10px 0 10px 60px;display:block;color:#333;border:1px solid #ccc}#yanzhengma{width:160px;border-radius:3px;-moz-border-radius:3px;margin-bottom:15px;outline:0;background:url(../Image/adminLoadIcon.png) 0 -108px no-repeat #eee}#checkNum{top:147px;right:39px}#userNum{background:url(../Image/adminLoadIcon.png) 0 -5px no-repeat #eee}#userPwd{background:url(../Image/adminLoadIcon.png) 0 -58px no-repeat #eee}#login{box-shadow:0 0 2px #fff inset;text-shadow:1px 1px 1px #fbe5ac;border-radius:3px;border:1px solid #e6b650;padding:8px 40px;font-size:20px;font-weight:400;right:39px;top:250px;z-index:999;background:#febf4d;background:linear-gradient(#ffd974,#febf4d);color:#986a39;font-family:"Microsoft Yahei","微软雅黑",arial,"宋体"}#login:focus,#login:hover{background-color:#ffd974;background-image:linear-gradient(#febf4d,#ffd974);border:1px solid #c1913d}form div a.forget{font-size:14px;top:210px;right:39px;color:#4C8CC9}div.autoDiv{margin:76px 0 5px 20px}label{font-size:16px}#error{top:206px;left:56px;padding-left:25px;background:url(../Image/adminIconList.png) 0 -372px no-repeat;height:24px;line-height:24px;color:#DD4B35}div.noLoignPage{width:300px;margin:50px auto 0;background:#fff;padding:20px 50px;text-align:center}p.noLoginTips{line-height:70px;height:70px;padding-left:70px;background:url(../Image/iconfont-jinzhi.png) no-repeat}p.noLoginTipsLink{margin-top:40px}p.noLoginTipsLink a{color:#1169bf}