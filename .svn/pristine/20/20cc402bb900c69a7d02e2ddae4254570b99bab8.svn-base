a#logo,a#toIndex,div#nav a,div#nav dl{display:block}#afooter p,div#nav h2,p.textCenter{text-align:center}*{margin:0;padding:0;border:0}body{font-family:"Microsoft Yahei",arial,"宋体";font-size:16px;width:100%;background:url(../Image/body_repeat.png) repeat-x #d5d5e1}a{text-decoration:none}a#name:hover,a.hasNews:hover,a.noNews:hover,div.top a:hover,p.textCenter a:hover{text-decoration:underline}ul{list-style:none}#aheader{height:100px;width:100%;background:#1169bf}#mycenter{width:900px;margin:0 auto}#aheader #mycenter{position:relative;padding-top:18px}#aheader,#aheader a{color:#fff}a#logo{height:60px;line-height:60px;padding-left:75px;font-size:30px;background:url(../Image/adminIconList.png) no-repeat;width:300px}div.atopright{position:absolute;right:0;top:43px}div.atopright span{background:url(../Image/adminIconList.png) 0 -66px no-repeat;padding-left:33px}a#exit{background:url(../Image/adminIconList.png) 0 -90px no-repeat #f36e6e;border-radius:6px;padding:2px 8px 2px 29px;margin-left:10px}a.hasNews,a.noNews{font-size:14px;padding:0 10px}a:hover#exit{background:url(../Image/adminIconList.png) 0 -116px no-repeat #f36e6e}a.hasNews{background:url(../Image/adminIconList.png) 53px -610px no-repeat}#acontent #mycenter{position:relative}#acontent{width:100%;background:url(../Image/body_bg.jpg) center top repeat-x #d5d5e1}#acontent div.top{padding:18px 0}#acontent div.top span{padding-left:28px;color:#252b3b;background:url(../Image/adminIconList.png) 0 -143px no-repeat}div.top a{color:#1169bf}a#toIndex{position:absolute;right:2px;top:13px;height:31px;line-height:31px;padding-left:36px;background:url(../Image/adminIconList.png) 0 -163px no-repeat}div#acontent{overflow:hidden;padding-bottom:20px}div#nav{width:200px;float:left;background:rgba(105,107,115,.5);filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr=#7F696b73,
		endcolorstr=#7F696b73);color:#fff;padding-bottom:20px;height:945px}div#nav h2{height:55px;line-height:55px;background:#1169bf;font-weight:100}div#nav h3{font-size:18px;font-weight:100;padding:5px 10px 5px 40px}div#nav ul h3{background:url(../Image/AddAndMinus.gif) 13px -35px no-repeat}div#nav h3:hover{cursor:pointer;background:url(../Image/AddAndMinus.gif) 13px -35px no-repeat #779dc7}div#nav a{color:#fff;height:25px;line-height:25px;padding-left:50px}div#nav a:hover{background:#9bafcc}div#main{width:655px;padding:10px;float:right;background:#fff;min-height:945px}#afooter{background:#1169bf;color:#fff;padding:10px 0;clear:both}p.fail,p.succeed{font-size:20px;height:40px;line-height:40px;padding-left:45px;margin:20px 0 20px 250px}p.succeed{background:url(../Image/adminIconList.png) 0 -267px no-repeat;color:green}p.fail{background:url(../Image/adminIconList.png) 0 -308px no-repeat;color:red}p.pmargin{margin-bottom:10px}p.textCenter a{color:#1169BF}.submitButton{box-shadow:0 0 2px #fff inset;text-shadow:1px 1px 1px #fbe5ac;border-radius:3px;border:1px solid #e6b650;padding:5px 15px;font-size:20px;font-weight:400;background:#febf4d;background:linear-gradient(#ffd974,#febf4d);color:#614323}.submitButton:focus,.submitButton:hover{background-color:#ffd974;background-image:linear-gradient(#febf4d,#ffd974);border:1px solid #c1913d}.addNavBg,.minusNavBg{background:url(../Image/AddAndMinus.gif) 10px 0 no-repeat}