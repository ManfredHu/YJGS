@CHARSET "UTF-8";

caption {
	padding: 5px;
	font-size: 20px;
}

thead {
	background: #1169bf;
	color: #fff;
	text-align: center;
}

thead th {
	padding: 2px 0;
}

div#main td {
	text-align: center;
}

th.number {
	width: 100px;
}

th.typeName {
	width: 130px;
}

th.productNum {
	width: 100px;
}

th.modify {
	width: 150px;
}

th.delete {
	width: 100px;
}

tbody td {
	padding: 3px 5px;
}
tbody td.deleteTd a {
	padding-left: 25px;
	background: url(../Image/adminIconList.png) no-repeat -6px -574px;
	color:#1169BF;
}
input.modify{
	
}
.textField{
	border: 1px solid #56abe4;
    font-size: 14px;
    padding: 5px 4px;
    width: 110px;
}
table.center{
	width:500px;
	margin:0px auto;
}

input#cancelBtn {
	padding: 3px 8px;
	background: #f36e6e;
	color: #fff;
	float: right;
	background: linear-gradient(#f36e6e, #ce5050) repeat;
	border-radius: 3px;
	box-shadow: 0 0 2px #fff inset;
	margin-right: 10px;
	font-size: 14px;
}

input#cancelBtn:hover {
	background: linear-gradient(#ce5050, #f36e6e) repeat;
}

input#sureBtn {
	padding: 3px 8px;
	background: #58C258;
	color: #fff;
	float: left;
	background: linear-gradient(#58C258, #2e9f2e) repeat;
	border-radius: 3px;
	box-shadow: 0 0 2px #fff inset;
	margin-left: 10px;
	font-size: 14px;
}

input:hover#sureBtn {
	background: linear-gradient(#2e9f2e, #58C258) repeat;
}

input#typeNameText {
	width: 110px;
	padding: 5px 4px;
	border: 1px solid #56abe4;
	font-size: 14px;
}

tbody td.modifyTd a {
	padding-left: 25px;
	background: url(../Image/adminIconList.png) no-repeat -6px -540px;
}