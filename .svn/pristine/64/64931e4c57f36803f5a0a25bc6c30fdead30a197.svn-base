package com.yjgs.dcl;

import java.sql.Date;

/**
 * 新闻列表展示实体类
 * @author 周宇钦
 *
 */
public class NewsList {
	
	private int 		newsID				=0;			//新闻ID
	
	private int		typeID				= 0;			//类别ID
	
	private String 		newsTitle			=null;		//新闻标题
	
	private Date 		newsPublishTime		=null;		//发布日期

	public int getNewsID() {
		return newsID;
	}

	public void setNewsID(int newsID) {
		this.newsID = newsID;
	}

	public int getTypeID() {
		return typeID;
	}

	public void setTypeID(int typeID) {
		this.typeID = typeID;
	}

	public String getNewsTitle() {
		return newsTitle;
	}

	public void setNewsTitle(String newsTitle) {
		this.newsTitle = newsTitle;
	}

	public Date getNewsPublishTime() {
		return newsPublishTime;
	}

	public void setNewsPublishTime(Date newsPublishTime) {
		this.newsPublishTime = newsPublishTime;
	}

	
	
	


}
