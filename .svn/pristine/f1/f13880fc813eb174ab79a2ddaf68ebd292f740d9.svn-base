@CHARSET "UTF-8";

form#typeOptions {
	height: 45px;
}

form#typeOptions span {
	float: left;
	height: 45px;
	line-height: 45px;
	padding-right: 10px;
	text-align: right;
	width: 280px;
}

form#typeOptions select {
	width: 150px;
	border: 1px solid #ccc;
	margin-top: 6.5px;
	_margin-top: 11.5px;
	padding: 5px 5px;
	font-size: 16px;
	padding: 5px 5px;
}

table.tb {
	margin-top: 20px;
	width: 655px;
}

table.tb td {
	text-align: center;
}

table.tb thead th {
	padding: 2px 0px;
	backgroundï¼š
}

table.tb thead {
	color: #fff;
	text-align: center;
	background: none repeat scroll 0 0 #1169bf;
}

table.tb thead th {
	padding: 2px 0px;
}

table.tb a {
	color: #1169BF;
}

table.tb a:hover {
	text-decoration: underline;
}

/* table.tb tbody tr td {
	padding: 2px 0px;
} */
tbody tr {
	height: 30px;
}

th.delete {
	width: 75px;
}

input.deletebtn {
	background: url("../Image/adminIconList.png") no-repeat 0px -505px
		#EC4F4F;
	padding: 0 0 0 25px;
	color: #fff;
	width: 75px;
	height: 24px;
	line-height: 24px;
	text-align: center;
	border: none 0;
	_background: #f05858;
	_padding: 0;
}

input.deletebtn:hover, input.deletebtn:focus {
	background: url("../Image/adminIconList.png") no-repeat 0px -505px
		#f59292;
}

div#PageList {
	margin-top: 40px;
	overflow: hidden;
	margin-left: 20px;
}

div#PageList a {
	margin: 0px 5px;
	border: 1px solid #e1e2e3;
	cursor: pointer;
	display: block;
	float: left;
	height: 34px;
	width: 34px;
	line-height: 34px;
	text-align: center;
	color: #1169BF;
	border: 1px solid #e1e2e3;
}

div#PageList a:hover {
	background: #f2f8ff;
	border: 1px solid #38f;
}

form.changePageForm {
	margin: 10px 0px 0px 330px;
}

select.changePage {
	border: 1px solid #ccc;
	font-size: 16px;
	width: 45px;
}

input.sure {
	background: none repeat scroll 0 0 #ffdf75;
	line-height: 20px;
	margin: 10px 0 0 10px;
	padding: 2px 4px;
	height: 24px;
}

div#PageList a.upPage {
	width: 100px;
	background: url(../Image/iconfont-xiayi.png) no-repeat 15px 9px;
	padding-left: 8px;
	_background: none;
	_padding-left: 0px;
}

div#PageList a.upPage:hover {
	background: url(../Image/iconfont-xiayi.png) no-repeat 15px 9px #f2f8ff;
	_background: #f2f8ff;
	border: 1px solid #38f;
}

div#PageList a.downPage {
	width: 100px;
	padding-right: 8px;
	background: url(../Image/iconfont-xiayi.png) no-repeat 80px -41px;
	_background: none;
	_padding-right: 0px;
}

div#PageList a.downPage:hover {
	background: url(../Image/iconfont-xiayi.png) no-repeat 80px -41px
		#f2f8ff;
	_background: #f2f8ff;
	border: 1px solid #38f;
}